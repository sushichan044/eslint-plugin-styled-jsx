{
  "name": "eslint-plugin-styled-jsx",
  "version": "0.0.2",
  "type": "module",
  "description": "ESLint plugin for styled-jsx",
  "sideEffects": false,
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint plugin",
    "eslint-plugin",
    "styled-jsx",
    "styled-jsx eslint plugin",
    "react"
  ],
  "homepage": "https://github.com/sushichan044/eslint-plugin-styled-jsx#readme",
  "bugs": {
    "url": "https://github.com/sushichan044/eslint-plugin-styled-jsx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushichan044/eslint-plugin-styled-jsx.git"
  },
  "author": {
    "name": "sushichan044",
    "url": "https://github.com/sushichan044"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": "https://github.com/sponsors/sushichan044",
  "engines": {
    "node": ">=20.19.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --fix",
    "lint:check": "eslint --max-warnings 0",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "typecheck": "tsgo --noEmit",
    "test": "vitest --run",
    "check": "pnpm run format:check && pnpm run typecheck && pnpm run lint:check && pnpm run test && pnpm run build",
    "build": "tsdown",
    "prepublishOnly": "pnpm run build",
    "pkg-pr-new": "pkg-pr-new publish --compact --comment=update --pnpm",
    "docs:generate": "eslint-doc-generator"
  },
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "@typescript-eslint/utils": "^8.49.0"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "catalog:dev",
    "@types/node": "catalog:dev",
    "@typescript-eslint/typescript-estree": "8.49.0",
    "@typescript/native-preview": "catalog:dev",
    "@virtual-live-lab/eslint-config": "catalog:dev",
    "@virtual-live-lab/tsconfig": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-doc-generator": "2.4.0",
    "eslint-plugin-eslint-plugin": "7.2.0",
    "eslint-plugin-import-access": "catalog:dev",
    "eslint-vitest-rule-tester": "3.0.0",
    "oxfmt": "catalog:dev",
    "pkg-pr-new": "catalog:dev",
    "publint": "catalog:dev",
    "release-it": "catalog:dev",
    "release-it-pnpm": "catalog:dev",
    "styled-jsx": "5.1.7",
    "tinyexec": "1.0.2",
    "tsdown": "catalog:dev",
    "type-fest": "5.3.1",
    "typescript": "catalog:dev",
    "typescript-eslint": "catalog:dev",
    "unplugin-unused": "catalog:dev",
    "vite": "catalog:dev",
    "vitest": "catalog:dev",
    "zimmerframe": "1.1.4"
  }
}
